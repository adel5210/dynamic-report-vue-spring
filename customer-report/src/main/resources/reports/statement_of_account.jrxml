	<subDataset name="DS_OUTSTAND_BAL" uuid="646baa32-343e-4d1c-86e8-1959d06b5cc2">
		<parameter name="customerId" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="terminationFee" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select (select sum(ohinvamt_gl)
from orderhdr_all
where customer_id = $P{customerId}
and ohstatus in ('IN','CM'))
 -
(select sum(cachkamt_gl)
from cashreceipts_all
where customer_id =  $P{customerId}
and catype not in (61,62) and cachkamt_gl != 0)
+
$P{terminationFee}
as totalOutStandBal from dual]]>
		</queryString>
		<field name="TOTALOUTSTANDBAL" class="java.math.BigDecimal"/>
	</subDataset>
